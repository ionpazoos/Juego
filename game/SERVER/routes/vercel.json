{
    "functions": {
        "routes/*.php": { "runtime": "vercel-php@0.6.1" }
    },
    "routes": [
        { "src": "/routes/get/(.*)", "methods": ["GET"], "dest": "/SERVER/routes/getAllClassic.php" },
        { "src": "/routes/post/(.*)", "methods": ["POST"], "dest": "/SERVER/routes/postClassic.php" },
        { "src": "/(.*)", "dest": "/SERVER/routes/getAllClassic.php" }
    ]
}
